<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane maxHeight="200.0" minHeight="300.0" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.filetransferapp.Controller">
   <children>
      <VBox alignment="CENTER" layoutX="150.0" layoutY="50.0" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Label text="start as" />
            <Button fx:id="clientButton" mnemonicParsing="false" onAction="#onClientClick" text="client" />
            <Button fx:id="serverButton" mnemonicParsing="false" onAction="#onServerClick" text="server" />
         </children>
      </VBox>
   </children>
</AnchorPane>
